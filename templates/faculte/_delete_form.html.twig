<form method="post" action="{{ path('app_faculte_delete', {'id': faculte.id}) }}">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ faculte.id) }}">
	<button class="btn" class="btn btn-danger" onclick="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
		<i class="fas fa-trash-alt"></i>
	</button>
</form>
