<form method="post" action="{{ path('app_etudiant_delete', {'id': etudiant.id}) }}">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ etudiant.id) }}">
	<button class="btn" class="btn btn-danger" onclick="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
		<i class="fas fa-trash-alt"></i></button>
</form>
